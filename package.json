{
  "name": "@boll/boll-repo",
  "version": "1.0.0",
  "private": true,
  "repository": "https://github.com/microsoft/boll",
  "license": "MIT",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "build": "lage build",
    "change": "beachball change",
    "checkchange": "beachball check",
    "clean": "lage clean",
    "docs": "lage docs",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "release": "beachball publish",
    "test": "lage run test"
  },
  "devDependencies": {
    "@types/baretest": "^2.0.0",
    "@types/node": "^16.0.0",
    "baretest": "^2.0.0",
    "beachball": "^2.51.0",
    "lage": "^2.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^3.0.2",
    "typedoc": "^0.19.1",
    "typedoc-plugin-markdown": "^2.4.2",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "resolutions": {
    "**/@types/glob/@types/minimatch": "^3.0.0",
    "**/@types/minimatch/minimatch": "^3.0.0",
    "@types/node": "^16.0.0"
  },
  "rationale": {
    "resolutions": {
      "**/@types/glob/@types/minimatch": "pin to installed minimatch version, not *",
      "**/@types/minimatch/minimatch": "pin to version matching other minimatch deps, not *"
    }
  }
}
